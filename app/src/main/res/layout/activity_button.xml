<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_button"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EA646A"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="id.sch.smktelkom_mlg.project.xirpl402112029.deuxappkk.ButtonActivity">

    <Button
        android:id="@+id/buttonJawa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:shadowColor="#212121"
        android:text="Jawa" />

    <Button
        android:id="@+id/buttonSumatera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:shadowColor="#212121"
        android:text="Sumatera" />

    <Button
        android:id="@+id/buttonKalimantan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:shadowColor="#212121"
        android:text="Kalimantan" />

    <Button
        android:id="@+id/buttonBali"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:shadowColor="#212121"
        android:text="Bali" />

    <Button
        android:id="@+id/buttonPapua"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:shadowColor="#212121"
        android:text="Papua" />

    <Button
        android:id="@+id/buttonSulawesi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:shadowColor="#212121"
        android:text="Sulawesi" />


</LinearLayout>
